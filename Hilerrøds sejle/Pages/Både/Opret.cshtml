    <div class="form-group">
        <label asp-for="NytBåd.SejlNummer" class="control-label"></label>
        <input asp-for="NytBåd.SejlNummer" class="form-control" />
        <span asp-validation-for="NytBåd.SejlNummer" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="NytBåd.BådLængde" class="control-label"></label>
